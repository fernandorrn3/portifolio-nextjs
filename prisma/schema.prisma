generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialActions"]
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")

}



model User {
  id            String    @id @default(cuid())
  name          String
  email         String    @unique
  senha         String
  emailVerified DateTime?
  isAdm         Boolean   @default(false)
  image         String?
 
}
